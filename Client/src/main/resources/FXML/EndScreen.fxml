<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EndScreenController">


    <Label id="title" fx:id="title" text="GAME OVER">
      <font>
         <Font size="50.0" />
      </font></Label>
    <Label id="result" fx:id="result" text="Victory/Defeat">
      <font>
         <Font size="40.0" />
      </font></Label>
    <Button id="gameMenu" fx:id="gameMenu" onAction="#menuButton" text="Game Menu">
      <font>
         <Font size="28.0" />
      </font></Button>

</VBox>
